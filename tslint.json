{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "trailing-comma": false,
    "max-line-length": {
      "options": [120]
    },
    "variable-name": {
      "options": ["ban-keywords", "check-format", "allow-leading-underscore", "allow-pascal-case"]
    }
  },
  "rulesDirectory": []
}
